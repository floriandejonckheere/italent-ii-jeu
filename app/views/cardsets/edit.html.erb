<div class="row row-animated">
  <div class="col-md-6 col-md-offset-3">
    <h2 class="list-title">
      <%= link_to cardsets_path, :class => "link-icon icon-xl" do %>
        <i class="mdi-navigation-arrow-back"></i>
      <% end %>
    <%= t('action_edit') %>
    </h2>
    <hr>
    <div class="panel panel-padding panel-default">
      <h4><%= @cardset.title %></h4>
      <div class="panel-body">
        <%= form_for @cardset, url: cardset_path(@cardset) do |f| %>
          <%= render 'form', f: f %>
        <% end %>
      </div>
    </div>
  </div>
</div>
